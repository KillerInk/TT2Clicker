<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/autolevelskills"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="Auto lvl Skills"
        custom:settingkey_ids="@string/autolvlskills"
        custom:setting_defaults="false"/>
    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/usehs"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="UseHS"
        custom:settingkey_ids="@string/useHs"
        custom:setting_defaults="false"/>
    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/useds"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="UseDS"
        custom:settingkey_ids="@string/useDS"
        custom:setting_defaults="false"/>

    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/usehom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="UseHom"
        custom:settingkey_ids="@string/useHom"
        custom:setting_defaults="false"/>
    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/usefs"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="UseFS"
        custom:settingkey_ids="@string/useFS"
        custom:setting_defaults="false"/>
    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/usewc"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="UseWC"
        custom:settingkey_ids="@string/useWC"
        custom:setting_defaults="false"/>
    <clickerbot.com.troop.clickerbot.views.SettingsSwitch
        android:id="@+id/usesc"
        android:layout_width="match_parent"
        android:layout_height="@dimen/settingitem_height"
        android:text="UseSC"
        custom:settingkey_ids="@string/useSC"
        custom:setting_defaults="false"/>
</LinearLayout>